<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  
  <!-- This path is relative the config file, not to the launch4j.exe -->
  <jar>target\slike-1.2.jar</jar>
  
  <!-- This path is relative the config file, not to the launch4j.exe -->
  <outfile>build\slike.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  <icon>program_icon.ico</icon>
  <jre>
  
	<!-- This path needs to be ralative to the generated slike.exe -->
	<!-- Since 'build' dir contains both 'slike.exe' and 'jre-17/' on the same level, looking 'from exe' path is only 'jre-17' -->
    <path>jre-17</path>
    <bundledJre64Bit>true</bundledJre64Bit>
    <bundledJreAsFallback>false</bundledJreAsFallback>
    <minVersion>1.8</minVersion>
    <maxVersion>17</maxVersion>
    <jdkPreference>jdkOnly</jdkPreference>
    <runtimeBits>64/32</runtimeBits>
  </jre>
</launch4jConfig>